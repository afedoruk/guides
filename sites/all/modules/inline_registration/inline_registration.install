/**
 * Implementation of hook_uninstall().
 */
function inline_registration_uninstall() {
  // Delete variables.
  foreach (node_get_types() as $type) {
    variable_del('inline_registration_'. $type->type);
    variable_del('inline_registration_weight_'. $type->type);
  }  
}
